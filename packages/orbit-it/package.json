{
  "name": "orbit-it",
  "version": "1.0.0-beta.1",
  "description": "Orbit It - A comprehensive toolkit for managing and interacting with Git and GitHub, including version control, issue tracking, and repository management.",
  "keywords": [],
  "author": {
    "name": "fermeridamagni <Magni Development>",
    "email": "hello@magni.dev",
    "url": "https://magni.dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fermeridamagni/orbit-it.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/fermeridamagni/orbit-it/issues",
    "email": "help@magni.dev"
  },
  "homepage": "https://github.com/fermeridamagni/orbit-it",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "bin": {
    "orbit-it": "./build/cli.cjs"
  },
  "exports": {
    ".": {
      "import": "./build/cli.mjs",
      "require": "./build/cli.cjs"
    },
    "./config": {
      "import": {
        "types": "./build/config.d.mts",
        "default": "./build/config.mjs"
      },
      "require": {
        "types": "./build/config.d.cts",
        "default": "./build/config.cjs"
      }
    }
  },
  "files": [
    "build",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "start": "node ./build/cli.cjs",
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@orbit-it/core": "workspace:*",
    "@clack/prompts": "^0.11.0",
    "commander": "^14.0.0",
    "fast-glob": "^3.3.3",
    "picocolors": "^1.1.1",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "unbuild": "^3.6.0",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.14.0"
}
